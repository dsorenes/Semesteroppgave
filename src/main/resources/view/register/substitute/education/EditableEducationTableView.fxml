<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.substitute.EditableEducationTableController">

    <TableView fx:id="educationTable" prefHeight="122.0" prefWidth="665.0">
        <columns>
            <TableColumn fx:id="schoolNameCol" prefWidth="75.0" text="School" />
            <TableColumn fx:id="degreeCol" prefWidth="75.0" text="Degree" />
            <TableColumn fx:id="subjectCol" prefWidth="75.0" text="Subject" />
            <TableColumn fx:id="educationLevelCol" prefWidth="87.0" text="Education Level" />
            <TableColumn fx:id="fromCol" prefWidth="70.0" text="From" />
            <TableColumn fx:id="toCol" prefWidth="75.0" text="To" />
            <TableColumn fx:id="isStudyingCol" prefWidth="75.0" text="Currently studying" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

</AnchorPane>
